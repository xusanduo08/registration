{
  "extends": "airbnb",
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "requireConfigFile": false,
    "babelOptions": {
      "presets": ["@babel/preset-react"]
    },
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    }
  },
  "rules": {
    "import/no-extraneous-dependencies": 0,
    "react/prop-types": 0,
    "react/jsx-filename-extension": 0,
    "react/react-in-jsx-scope": 0
  }
}
